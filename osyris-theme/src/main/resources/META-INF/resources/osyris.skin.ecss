
html, body, div, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center, dl, dt, dd,
fieldset, form, label, legend, caption, figcaption,
header, footer, article, section, nav, menu, hgroup {
	margin:0;
	padding:0;
	border:0;
	outline:0;
	/*font-size:100%;*/
	background:transparent;
}

.page-normal-body {
    background-color: #4988CA;
    background-image: "url(#{resource['osyris/images/clouds.png']})";
    background-position: center top;
    background-repeat: no-repeat;
    font-family: "Arial,Helvetica,sans-serif";
}

.page-normal-container {
    background-color: #FFFFFF;
    background-image: "url(#{resource['osyris/images/circles.png']})";
    background-position: right top;
    background-repeat: no-repeat;
    box-shadow: 0 5px 15px rgba(50, 50, 50, 0.5);
    margin: auto;
    position: relative;
    width: 1125px;
}

.page-footer {
	width:1125px;
	height:300px;
	padding: 0;
	border: none;
	border-top:8px solid #57891b;
	background-color:#73a42b;
	background-image: "url(#{resource['osyris/images/bush.jpg']})";
	background-position:top;
	background-repeat:no-repeat;
}

.page-footer .address {
	text-align:center;
	margin-top:25px;
	color:#fff;
	font-size:1.2em;
}

.page-footer .address a {
	color:#fff;
}

.header .header_logo {
	margin-top:25px;
	margin-left:100px;
}

.page-footer .toerisme_oost_vlaanderen {
	margin-top:25px;
}

.model-field-label {
	display: block;
	float: left;
	width: 100px;
	vertical-align: baseline;
}

.component-map-featureInfo-result .model-field-value {
	margin-left: 205px;
}

.component-map-featureInfo-result .model-field-label {
	display: block;
	float: left;
	width: 200px;
	vertical-align: baseline;
}

.model-field-value {
	margin-left: 105px;
}

.model-field-tail {
	clear: both;
}

.icn_user {
	background: url("#{resource['osyris/images/icn_user.png']}") 20px 50% no-repeat;
}

.icn_stop {
	background: url("#{resource['osyris/images/icn_stop.png']}") 20px 50% no-repeat;
}

.account {
	width:480px;
	height:50px;
	position:absolute;
	top:0;
	right:0;
	padding-right: 20px;
}
			
	.account ul {
		float:right;
		height:50px;
		margin:0;
		padding:0;
		background-color:#73a42b;
		list-style-type:none;
	}
		.account ul li {
			float:left;
			padding:0 20px 0 50px;
			line-height:50px;
			font-size:1.2em;
			color:#fff;
		}
					
			.account a {
				color:#cee6ab;
			}
				
			.account .stop a{
				color:#fff;
			}
						
			.account .help {
				width:50px;
				padding:12px 4px 13px 4px;
				background-color:#598b1c;
			}
						
			.account .help .question_mark {
				width:25px;
				height:25px;
				margin:auto;
				display:block;
				line-height:25px;
				text-align:center;
				background-color:#fff;
				border-radius:1em;
				color:#457010;
				font-size:1.2em;
				text-decoration:none;
			}
.header .menu{
	width:1000px;
	height:50px;
	margin:auto;
	padding-left: 20px;
}
		
	.menu ul{
		height:50px;
		margin:0;
		padding:0;
		display:inline-block;
		list-style-type:none;
		background: #689b26; /* Old browsers */
		/* IE9 SVG, needs conditional override of 'filter' to 'none' */
		background: url(data:image/svg+xml;base64,);
		background: -moz-linear-gradient(left, #689b26 0%, #9dc53b 100%); /* FF3.6+ */
		background: -webkit-gradient(linear, left top, right top, color-stop(0%,#689b26), color-stop(100%,#9dc53b)); /* Chrome,Safari4+ */
		background: -webkit-linear-gradient(left, #689b26 0%,#9dc53b 100%); /* Chrome10+,Safari5.1+ */
		background: -o-linear-gradient(left, #689b26 0%,#9dc53b 100%); /* Opera 11.10+ */
		background: -ms-linear-gradient(left, #689b26 0%,#9dc53b 100%); /* IE10+ */
		background: linear-gradient(to right, #689b26 0%,#9dc53b 100%); /* W3C */
		filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#689b26', endColorstr='#9dc53b',GradientType=1 ); /* IE6-8 */
	}
			
		.menu ul li{
			float:left;
				border-right:2px solid #fff;
			}
			.menu ul li a{
				height:50px;
				display:block;
				padding:0 12px;
				line-height:50px;
				color:#fff;
				font-size:1.2em;
				text-decoration:none;
			}
			.menu ul li a.active{
				height:50px;
				display:block;
				padding:0 12px;
				line-height:50px;
				color:#000;
				font-size:1.2em;
				text-decoration:none;
				background-color:#f3f3f3;
				border-top:2px solid #be3339;
			}
			
